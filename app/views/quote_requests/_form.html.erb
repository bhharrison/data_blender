<%= simple_form_for(@quote_request) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :website %>
    <%= f.input :callback %>
    <%= f.input :description %>
    <%= f.input :hours %>
    <%= f.input :cost %>
    <%= f.input :nte %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

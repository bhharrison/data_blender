<%- model_class = QuoteRequest -%>
<div class="page-header">
  <h1><%=t '.title', :default => QuoteRequest %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:website) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request.website %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:callback) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request.callback %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:hours) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request.hours %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cost) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request.cost %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nte) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request.nte %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              quote_requests_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_quote_request_path(@quote_request), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              quote_request_path(@quote_request),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

